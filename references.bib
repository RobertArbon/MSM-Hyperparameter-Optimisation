@article{kells_correlation_2020,
	title = {Correlation functions, mean first passage times, and the Kemeny constant},
	volume = {152},
	issn = {0021-9606},
	url = {https://aip.scitation.org/doi/full/10.1063/1.5143504},
	doi = {10.1063/1.5143504},
	abstract = {Markov processes are widely used models for investigating kinetic networks. Here, we collate and present a variety of results pertaining to kinetic network models in a unified framework. The aim is to lay out explicit links between several important quantities commonly studied in the field, including mean first passage times ({MFPTs}), correlation functions, and the Kemeny constant. We provide new insights into (i) a simple physical interpretation of the Kemeny constant, (ii) a relationship to infer equilibrium distributions and rate matrices from measurements of {MFPTs}, and (iii) a protocol to reduce the dimensionality of kinetic networks based on specific requirements that the {MFPTs} in the coarse-grained system should satisfy. We prove that this protocol coincides with the one proposed by Hummer and Szabo [J. Phys. Chem. B 119, 9029 (2014)], and it leads to a variational principle for the Kemeny constant. Finally, we introduce a modification of this protocol, which preserves the Kemeny constant. Our work underpinning the theoretical aspects of kinetic networks will be useful in applications including milestoning and path sampling algorithms in molecular simulations.},
	pages = {104108},
	number = {10},
	journaltitle = {The Journal of Chemical Physics},
	shortjournal = {J. Chem. Phys.},
	author = {Kells, Adam and Koskin, Vladimir and Rosta, Edina and Annibale, Alessia},
	urldate = {2021-06-16},
