	number = {1},
	journaltitle = {Nature Computational Science},
	shortjournal = {Nat Comput Sci},
	author = {Löhr, Thomas and Kohlhoff, Kai and Heller, Gabriella T. and Camilloni, Carlo and Vendruscolo, Michele},
