@article{Mosen2004,
	title = {Particle formation and capture during spray drying of inhalable particles},
	volume = {9},
	issn = {10837450},
	doi = {10.1081/PDT-200035795},
	abstract = {An investigation of the spray drying process is made in great detail regarding particle formation and capture efficiency with focus on the production of inhalable particles. Mannitol was spray dried as model substance and the spray-dried products were characterized. The resulting products consisted of smooth spheres with a volume median diameter of 2.2-5.5 microm, and narrow size distributions. The investigation was performed in pilot scale of sufficient size to draw general conclusions and make some recommendations. It has been shown that the size of particles is decreased when the feed concentration is decreased, the nozzle gas/feed flow mass ratio increased, and the droplet size decreased. The collection efficiency of the cyclone device used in this study was shown to have a cut-off of 2 microm, i.e., 50\% of the particles less than 2 microm are not captured. The data reported indicate that the majority of the single particles formed here, {\textless}5 microm, arise from single droplets (of about 10 microm) and are solid, nonporous particles.},
	pages = {409--417},
	number = {4},
	journaltitle = {Pharmaceutical Development and Technology},
