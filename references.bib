@article{varutbangkul_hygroscopicity_2006,
	title = {Hygroscopicity of secondary organic aerosols formed by oxidation of cycloalkenes, monoterpenes, sesquiterpenes, and related compounds},
	volume = {6},
	issn = {1680-7316},
	url = {https://acp.copernicus.org/articles/6/2367/2006/},
	doi = {10.5194/acp-6-2367-2006},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-{contentHeaderColor}"{\textgreater}Abstract.{\textless}/strong{\textgreater} A series of experiments has been conducted in the Caltech indoor smog chamber facility to investigate the water uptake properties of aerosol formed by oxidation of various organic precursors. Secondary organic aerosol ({SOA}) from simple and substituted cycloalkenes (C$_{\textrm{5}}$-C$_{\textrm{8}}$) is produced in dark ozonolysis experiments in a dry chamber (\textit{{RH}}{\textasciitilde}5\%). Biogenic {SOA} from monoterpenes, sesquiterpenes, and oxygenated terpenes is formed by photooxidation in a humid chamber ({\textasciitilde}50\% \textit{{RH}}). Using the hygroscopicity tandem differential mobility analyzer ({HTDMA}), we measure the diameter-based hygroscopic growth factor (\textit{{GF}}) of the {SOA} as a function of time and relative humidity. All {SOA} studied is found to be slightly hygroscopic, with smaller water uptake than that of typical inorganic aerosol substances. The aerosol water uptake increases with time early in the experiments for the cycloalkene {SOA}, but decreases with time for the sesquiterpene {SOA}. This behavior could indicate competing effects between the formation of more highly oxidized polar compounds (more hygroscopic), and formation of longer-chained oligomers (less hygroscopic). All {SOA} also exhibit a smooth water uptake with \textit{{RH}} with no deliquescence or efflorescence. The water uptake curves are found to be fitted well with an empirical three-parameter functional form. The measured pure organic \textit{{GF}} values at 85\% \textit{{RH}} are between 1.09\&ndash;1.16 for {SOA} from ozonolysis of cycloalkenes, 1.01\&ndash;1.04 for sesquiterpene photooxidation {SOA}, and 1.06\&ndash;1.10 for the monoterpene and oxygenated terpene {SOA}. The \textit{{GF}} of pure {SOA} (\textit{{GF}}$_{\textrm{org}}$) in experiments in which inorganic seed aerosol is used is determined by assuming volume-weighted water uptake (Zdanovskii-Stokes-Robinson or "{ZSR}" approach) and using the size-resolved organic mass fraction measured by the Aerodyne Aerosol Mass Spectrometer. Knowing the water content associated with the inorganic fraction yields \textit{{GF}}$_{\textrm{org}}$ values. However, for each precursor, the \textit{{GF}}$_{\textrm{org}}$ values computed from different {HTDMA}-classified diameters agree with each other to varying degrees. Comparing growth factors from different precursors, we find that \textit{{GF}}$_{\textrm{org}}$ is inversely proportional to the precursor molecular weight and {SOA} yield, which is likely a result of the fact that higher-molecular weight precursors tend to produce larger and less hygroscopic oxidation products.{\textless}/p{\textgreater}},
	pages = {2367--2388},
	number = {9},
	journaltitle = {Atmospheric Chemistry and Physics},
	author = {Varutbangkul, V. and Brechtel, F. J. and Bahreini, R. and Ng, N. L. and Keywood, M. D. and Kroll, J. H. and Flagan, R. C. and Seinfeld, J. H. and Lee, A. and Goldstein, A. H.},
