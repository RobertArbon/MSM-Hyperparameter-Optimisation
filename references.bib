	author = {Hansen, Katja and Montavon, Grégoire and Biegler, Franziska and Fazli, Siamac and Rupp, Matthias and Scheffler, Matthias and Von Lilienfeld, O. Anatole and Tkatchenko, Alexandre and Müller, Klaus Robert},
