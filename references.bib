@article{cardenas_unassisted_2012,
	title = {Unassisted Transport of N-acetyl-L-tryptophanamide through Membrane: Experiment and Simulation of Kinetics},
	volume = {116},
	issn = {1520-6106},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3302722/},
	doi = {10.1021/jp2102447},
	shorttitle = {Unassisted Transport of N-acetyl-L-tryptophanamide through Membrane},
	abstract = {Cellular transport machinery, such as channels and pumps, is working against the background of unassisted material transport through membranes. The permeation of a blocked tryptophan through a 1,2-Dioleoyl-sn-glycero-3-phosphocholine ({DOPC}) membrane is investigated to probe unassisted or physical transport. The transport rate is measured experimentally and modeled computationally. The time scale measured by Parallel Artificial Membrane Permeation Assay ({PAMPA}) experiments is {\textasciitilde}8 h. Simulations with the Milestoning algorithm suggest Mean First Passage Time ({MFPT}) of {\textasciitilde}4 h and the presence of a large barrier at the center of the bilayer. A similar calculation with the solubility-diffusion model yields {MFPT} of {\textasciitilde}15 min. This permeation rate is nine orders of magnitude slower than the permeation rate of only a tryptophan side chain (computed by us and others). This difference suggests critical dependence of transport time on permeant size and hydrophilicity. Analysis of the simulation results suggests that the permeant partially preserves hydrogen bonding of the peptide backbone to water and lipid molecules even when it is moving closer to the bilayer center. As a consequence, defects of the membrane structure are developed to assist permeation.},
	pages = {2739--2750},
	number = {9},
	journaltitle = {The Journal of Physical Chemistry. B},
	shortjournal = {J Phys Chem B},
	author = {Cardenas, Alfredo E. and Jas, Gouri S. and {DeLeon}, Kristine Y. and Hegefeld, Wendy A. and Kuczera, Krzysztof and Elber, Ron},
	urldate = {2021-04-21},
	date = {2012-03-08},
	pmid = {22313494},
