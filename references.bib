	author = {Researcher, Juan and Abade, Brazil and Landscape Ecologist Australia Abd Khalid, Brett and Salwa, Siti and Abdallah, Argentina and Student, Mohamad and Aziz, Abdul and Rimba Malaysia, Sheema and William Ripple, by J and Wolf, Christopher and Galetti, Mauro and Newsome, Thomas M and Alamgir, Mohammed and Crist, Eileen and Mahmoud, Mahmoud I and Laurance, William F},
