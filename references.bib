	url = {https://www.microbiologyresearch.org/content/journal/micro/10.1099/00221287-147-8-2195},
	doi = {10.1099/00221287-147-8-2195},
	abstract = {The nucleotide sequence of the aromatic amine utilization ({\textless}span class="jp-italic"{\textgreater}aau{\textless}/span{\textgreater}) gene region from {\textless}span class="jp-italic"{\textgreater}Alcaligenes faecalis{\textless}/span{\textgreater} contained nine genes ({\textless}span class="jp-italic"{\textgreater}orf-1{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}{aauBEDA}{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}orf-2{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}orf-3{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}orf-4{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{hemE}{\textless}/span{\textgreater}) transcribed in the same direction. The {\textless}span class="jp-italic"{\textgreater}{aauB}{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{aauA}{\textless}/span{\textgreater} genes encode the periplasmic aromatic amine dehydrogenase ({AADH}) large and small subunit polypeptides, respectively, and were homologous to {\textless}span class="jp-italic"{\textgreater}{mauB}{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{mauA}{\textless}/span{\textgreater}, the genes for the large and small subunits of methylamine dehydrogenase ({MADH}). {\textless}span class="jp-italic"{\textgreater}{aauE}{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{aauD}{\textless}/span{\textgreater} are homologous to {\textless}span class="jp-italic"{\textgreater}{mauE}{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{mauD}{\textless}/span{\textgreater} and apparently carry out the same function of transport and folding of the small subunit polypeptide in the periplasm. No analogues of the {\textless}span class="jp-italic"{\textgreater}{mauF}{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}{mauG}{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}{mauL}{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}{mauM}{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}{mauN}{\textless}/span{\textgreater} genes responsible for biosynthesis of tryptophan tryptophylquinone (the prosthetic group of amine dehydrogenases) were found in the {\textless}span class="jp-italic"{\textgreater}aau{\textless}/span{\textgreater} cluster. {\textless}span class="jp-italic"{\textgreater}orf-2{\textless}/span{\textgreater} was predicted to encode a small periplasmic monohaem {\textless}span class="jp-italic"{\textgreater}c{\textless}/span{\textgreater}-type cytochrome. No biological function can be assigned to polypeptides encoded by {\textless}span class="jp-italic"{\textgreater}orf-1{\textless}/span{\textgreater}, {\textless}span class="jp-italic"{\textgreater}orf-3{\textless}/span{\textgreater} and {\textless}span class="jp-italic"{\textgreater}orf-4{\textless}/span{\textgreater} and mutations in these genes appeared to be lethal. Mutants generated by insertions into {\textless}span class="jp-italic"{\textgreater}{mauD}{\textless}/span{\textgreater} were not able to use phenylethylamine, tyramine and tryptamine as a source of carbon and phenylethylamine, 3’-hydroxytyramine (dopamine) and tyramine as a source of nitrogen, indicating that {AADH} is the only enzyme involved in utilization of primary amines in {\textless}span class="jp-italic"{\textgreater}A. faecalis{\textless}/span{\textgreater}. {AADH} genes are present in {\textless}span class="jp-italic"{\textgreater}Alcaligenes xylosoxydans{\textless}/span{\textgreater} subsp. {\textless}span class="jp-italic"{\textgreater}xylosoxydans{\textless}/span{\textgreater}, but not in other β- and γ-proteobacteria. Phylogenetic analysis of amine dehydrogenases ({MADH} and {AADH}) indicated that {AADH} and {MADH} evolutionarily diverged before separation of proteobacteria into existing subclasses.,},
	pages = {2195--2202},
	number = {8},
	journaltitle = {Microbiology,},
	author = {Chistoserdov, Andrei Y},
	urldate = {2020-08-10},
	date = {2001},
	note = {Publisher: Microbiology Society,},
}

@article{hyun_electron_1995,
	title = {Electron Transfer Reactions between Aromatic Amine Dehydrogenase and Azurin},
	volume = {34},
	issn = {0006-2960, 1520-4995},
	url = {https://pubs.acs.org/doi/abs/10.1021/bi00038a020},
	doi = {10.1021/bi00038a020},
	abstract = {Binding and electron transfer reactions between the tryptophan tryptophylquinone ({TTQ}) enzyme, aromatic amine dehydrogenase ({AADH}), and the type I copper protein azurin have been characterized. In steady-state kinetic assays using azurin as an electron acceptor, it was observed that the apparent Km for azurin decreased with increasing ionic strength. These results are the opposite of what was observed for the reaction between the {TTQ} enzyme methylamine dehydrogenase ({MADH}) and amicyanin, despite the fact that in both cases the pairs of redox proteins are each acidic proteins. It was further demonstrated that azurin does not function as an effective electron acceptor for {MADH}, and that amicyanin does not function as an effective electron acceptor for {AADH}. Thus, while the two {TTQ} enzymes each use type I copper proteins as physiologic electron acceptors, there is a strong specificity for which copper protein serves as a redox partner. The kinetic parameters for the electron transfer reactions from reduced {AADH} to oxidized azurin were determined by stopped-flow spectroscopy. Different results were obtained depending upon whether {AADH} was reduced chemically with dithionite or with the substrate tyramine. The values for the limiting first-order apparent electron transfer rate constant (\&{ET}) at 30 °C were 4 and 102 s\_1, respectively. Kinetically determined values of Kd also differed by a factor of 2.4. These data suggest that the incorporation of the substrate-derived amino group into the reduced {TTQ} of {AADH} significantly increases the apparent \& · The interaction between {AADH} and azurin was also quantitated using an ultrafiltration binding assay. This yielded a Kd of 300 µ for the {AADH}—azurin complex. This Kd correlated well with the kinetically determined Kd values obtained from the stoppedflow kinetic studies. Similarities and differences i the reactivities of the {AADH}-azurin and {MADHamicyanin} redox pairs are discussed.},
	pages = {12249--12254},
	number = {38},
	journaltitle = {Biochemistry},
	shortjournal = {Biochemistry},
	author = {Hyun, Young-Lan and Davidson, Victor L.},
	urldate = {2020-08-10},
	date = {1995-09-26},
	langid = {english},
}

