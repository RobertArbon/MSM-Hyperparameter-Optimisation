@article{banavali_electrostatic_2002,
	title = {Electrostatic free energy calculations using the generalized solvent boundary potential method},
	volume = {117},
	issn = {00219606},
	doi = {10.1063/1.1507108},
	abstract = {Free energy perturbation ({FEP}) calculations using all-atom molecular\${\textbackslash}backslash\$ndynamics simulations with a large number of explicit solvent molecules\${\textbackslash}backslash\$nare a powerful approach to study ligand-macromolecule association\${\textbackslash}backslash\$nprocesses at the atomic level. One strategy to carry out {FEP} calculations\${\textbackslash}backslash\$nefficiently and reduce computational time is to consider the explicit\${\textbackslash}backslash\$ndynamics of only a small number of atoms in a localized region around\${\textbackslash}backslash\$nthe ligand. Such an approximation is motivated by the observation\${\textbackslash}backslash\$nthat the factors governing binding specificity are dominated by interactions\${\textbackslash}backslash\$nin the vicinity of the ligand. However, a straightforward truncation\${\textbackslash}backslash\$nof the system may yield inaccurate results as the influence exerted\${\textbackslash}backslash\$nby the remote regions of the macromolecule and the surrounding solvent\${\textbackslash}backslash\$nthrough long-range electrostatic effects may be significant. To obtain\${\textbackslash}backslash\$nmeaningful results, it is important to incorporate the influence\${\textbackslash}backslash\$nof the remote regions of the ligand-macromolecule complex implicitly\${\textbackslash}backslash\$nusing some effective potential. The generalized solvent boundary\${\textbackslash}backslash\$npotential ({GSBP}) that was developed recently [W. Im, S. Berneche,\${\textbackslash}backslash\$nand B. Roux, J. Chem. Phys. 114, 2924 (2001)] is an efficient computational\${\textbackslash}backslash\$nmethod to represent the long-range electrostatic interactions arising\${\textbackslash}backslash\$nfrom remote (outer) regions in simulations of a localized (inner)\${\textbackslash}backslash\$nregion with a small number of explicit atoms. In the present work,\${\textbackslash}backslash\${nFEP} calculations combined with {GSBP} are used to illustrate the importance\${\textbackslash}backslash\$nof these long-range electrostatic factors in estimation of the charging\${\textbackslash}backslash\$nfree energy of an aspartate ligand bound to the aspartyl-{tRNA} synthetase.\${\textbackslash}backslash\${nCalculations} with explicit spherical simulation inner regions of\${\textbackslash}backslash\$ndifferent radii are used to test the accuracy of the {GSBP} method\${\textbackslash}backslash\$nand also illustrate the importance of explicit protein and solvent\${\textbackslash}backslash\$ndynamics in the free energy estimation. The influence of the represented\${\textbackslash}backslash\$nouter region is tested using separate simulations in which the reaction\${\textbackslash}backslash\$nfield and/or the protein static field are excluded. Both components\${\textbackslash}backslash\$nare shown to be essential to obtain quantitatively meaningful results.\${\textbackslash}backslash\${nThe} ability of implicitly treating the influence of protein fluctuations\${\textbackslash}backslash\$nin the outer region using a protein dielectric constant is examined.\${\textbackslash}backslash\${nIt} is shown that accurate charging free energy calculations can be\${\textbackslash}backslash\$nperformed for this system with a spherical region of 15 to 20 A radius,\${\textbackslash}backslash\$nwhich roughly corresponds to 1500-3500 moving atoms. The results\${\textbackslash}backslash\$nindicate that {GSBP} in combination with {FEP} calculations is a precise\${\textbackslash}backslash\$nand efficient approach to include long-range electrostatic effects\${\textbackslash}backslash\$nin the study of ligand binding to large macromolecules. (C) 2002\${\textbackslash}backslash\${nAmerican} Institute of Physics.},
	pages = {7381--7388},
	number = {15},
	journaltitle = {Journal of Chemical Physics},
	author = {Banavali, Nilesh K. and Im, Wonpil and Roux, Benoît},
	date = {2002},
}

