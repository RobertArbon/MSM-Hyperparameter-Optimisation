@article{chistoserdov_cloning_2001,
	title = {Cloning, sequencing and mutagenesis of the genes for aromatic amine dehydrogenase from Alcaligenes faecalis and evolution of amine {dehydrogenasesThe} {GenBank} accession number for the aau gene cluster from Alcaligenes faecalis is {AF}302652.},
	volume = {147},
